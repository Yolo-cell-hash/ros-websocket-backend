<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ROS Data Display</title>
</head>
<body>
    <h1>ROS Data Display</h1>
    <p>Received data from ROS:</p>
    <ul>
        <% dataHistory.forEach(function(item, index) { %>
            <li><h4><%= JSON.stringify(item, null, 2) %></h4></li>
         <% }); %>
     </ul>
</body>
</html>
